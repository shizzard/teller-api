[doc]
User can get account data or 404 error

Check if we get 200 OK response with valid auth token and account_id
Check if we get 404 Not Found with valid auth token and invalid account_id
[enddoc]

[include ../../__common/common.luxinc]
[include ../../__common/http.luxinc]

[shell app]
  [invoke start-app]

[shell success]
  [progress (check success case)]
  [invoke http-call /accounts/$account_id $auth_token 200]
  ??"currency"
  ??"enrollment_id"
  ??"id"
  ??"institution"
  ??"id"
  ??"name"
  ??"last_four"
  ??"links"
  ??"balances"
  ??"self"
  ??"transactions"
  ??"name"
  ??"status"
  ??"subtype"
  ??"type"
  [invoke check-ok]

[shell not-found]
  [progress (check not found case)]
  [invoke http-call /accounts/acc_fakeaccount $auth_token 404]
  ??"code": "not_found"
  [invoke check-ok]

[shell app]
  [invoke stop-app]